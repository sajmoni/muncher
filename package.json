{
  "name": "muncher",
  "version": "0.0.10",
  "description": "Generate sprite sheets from the command line",
  "files": [
    "dist/"
  ],
  "directories": {
    "example": "example"
  },
  "scripts": {
    "clean": "rm -f muncher.tgz",
    "build-test": "yarn clean && yarn build && yarn pack --filename muncher.tgz && cd example && yarn refresh && yarn munch && cd -",
    "release": "yarn build && np --no-tests",
    "build": "rm -rf dist && parcel build ./index.js --target node --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/muncher.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bin": "dist/index.js",
  "dependencies": {
    "chalk": "3.0.0",
    "tmp": "0.1.0",
    "yargs": "15.3.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "np": "6.2.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-shebang": "1.2.8"
  }
}
